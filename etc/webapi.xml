            <resource ref="anonymous"/>
        </resources>
    </route>
    <route method="PUT" url="/V1/swedbank_pay_orders">
        <service class="SwedbankPay\Checkout\Api\OrderRepositoryInterface" method="save"/>
        <resources>
            <resource ref="anonymous"/>
        </resources>
    </route>
    <route method="DELETE" url="/V1/swedbank_pay_orders">
        <service class="SwedbankPay\Checkout\Api\OrderRepositoryInterface" method="delete"/>
        <resources>
            <resource ref="anonymous"/>
        </resources>
