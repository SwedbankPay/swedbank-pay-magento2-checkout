<?xml version="1.0"?>
<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Webapi:etc/webapi.xsd">
    <route method="GET" url="/V1/swedbank_pay_quotes/:id">
        <service class="SwedbankPay\Checkout\Api\QuoteRepositoryInterface" method="getById"/>
        <resources>
            <resource ref="anonymous"/>
        </resources>
    </route>
    <route method="GET" url="/V1/swedbank_pay_quotes">
        <service class="SwedbankPay\Checkout\Api\QuoteRepositoryInterface" method="getList"/>
        <resources>
            <resource ref="anonymouns"/>
        </resources>
    </route>
    <route method="POST" url="/V1/swedbank_pay_quotes">
        <service class="SwedbankPay\Checkout\Api\QuoteRepositoryInterface" method="save"/>
        <resources>
            <resource ref="anonymous"/>
        </resources>
    </route>
    <route method="PUT" url="/V1/swedbank_pay_quotes">
        <service class="SwedbankPay\Checkout\Api\QuoteRepositoryInterface" method="save"/>
        <resources>
            <resource ref="anonymous"/>
        </resources>
    </route>
    <route method="DELETE" url="/V1/swedbank_pay_quotes">
        <service class="SwedbankPay\Checkout\Api\QuoteRepositoryInterface" method="delete"/>
        <resources>
            <resource ref="anonymous"/>
        </resources>
    </route>

    <route method="GET" url="/V1/swedbank_pay_orders/:id">
        <service class="SwedbankPay\Checkout\Api\OrderRepositoryInterface" method="getById"/>
